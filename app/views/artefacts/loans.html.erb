<!-- Gmaps -->
<%= render :partial => "shared/location_item", :object => @artefact %>

<h1>Artefact Loan History</h1>
<p id="notice"><%= notice %></p>

<table>
	<tr>
		<td>
			<span class="artefact_id"><h3>Artefact <%= @artefact.artefactid %></h3></span>
		</td>
		<td colspan="2">
			Owner: <%= @artefact.user.userid %>
		</td>
	</tr>
	<%= render :partial => "artefact_item_long", :object => @artefact %>
</table>

<h3>History</h3>
<table class="loaner_info enum">
	<%= render :partial => "loans/loan_short", :collection => @loans %>
</table>
<%= will_paginate @loans %>